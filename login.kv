<Start>:
    MDBoxLayout:
        orientation: "vertical"
        outer_track_container: outer_track_container.__self__
        outer_tools_container: outer_tools_container.__self__
        #outer_track_container: album_info.__self__
        Widget:        	
        	pos: 0,0
        	layout: 'float'
        	height: '0dp'
        	size_hint: (None,None)


