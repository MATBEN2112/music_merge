# snackbar !!!
<AudioInfo>:
	id: audio_row
	orientation: 'vertical'
    #md_bg_color: "#232217"
    height:"105dp"
    size_hint: .96,None

    pos_hint: {'center_x':0.5, 'center_y':0.06}
	MDBoxLayout:
		height:"100dp"
        canvas.before:
            Color:
                rgba: (0,.7,.7,1)  # visual feedback of press
            RoundedRectangle:
                pos: self.pos
                size: self.size
                radius: [22,]

            Color:
                rgba: .1, .9, .1, 1
            Line:
                width: 1.5
                rounded_rectangle: (self.x, self.y, self.width, self.height, 22)

        MDRelativeLayout:
            size: ("100dp", "100dp")
            size_hint: (None,None)
		    Button:
                id: action
        	    background_color: 0,0,0,0

            Image:
                id: song_status
                size: ("80dp", "80dp")
                size_hint: (None,None)
                source: "./icons/play.png"
                pos_hint: {'center_x':0.5, 'center_y':0.5}

    				
		Button:
			id: song_name
			on_release: app.player.switch_to_player(self.text)
			background_color: 0,0,0,0

			size: root.size
			text: "Artist - song"
            font_size: '20sp'
            shorten: True
			
		Button:
			id: song_timer
			on_press: app.player.switch_to_player(root)
			size_hint: None,None
			background_color: 0,0,0,0
			size: "70dp","100dp"
			text: "00:00"
            font_size: '20sp'
			haling: "right"
            canvas.before:

                Color:
                    rgba: (0,0,0,0)  # visual feedback of press

                RoundedRectangle:
                    pos: self.pos
                    size: self.size
                    radius: [0,22,22,0]
                Color:
                    rgba: 0,1,0,1
                Line:
                    width: 1.5
                    points: self.pos[0],self.pos[1],self.pos[0],self.pos[1]+self.height

	MDProgressBar:
		id: song_progress
		size_hint_y:None
		height:"5dp"
		max: 1
		value: 1

<Album>
	MDBoxLayout:
        orientation: "vertical"
        MDRelativeLayout:
            id: menu_row
            orientation: "vertical"
            size: (root.size[0],"100dp")
