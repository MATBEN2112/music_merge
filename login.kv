                            height: "10dp"

                            canvas.before:

                                Color:
                                    rgba: (0,0,0,1)

                                Rectangle:
                                    pos: self.pos
                                    size: self.size

                    MDBoxLayout:
                        id: outer_track_container
                        size_hint: 1, None
                        height: self.minimum_height
                        MDList:
                            id: container


